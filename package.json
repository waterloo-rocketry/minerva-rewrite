{
  "name": "minerva-rewrite",
  "scripts": {
    "build": "tsc -p .",
    "format": "prettier --write 'src/**/*.{ts,tsx}' && eslint --fix 'src/**/*.{ts,tsx}'",
    "deploy-dev": "git push -f origin +@:development && echo 'View deployment at https://github.com/waterloo-rocketry/minerva-rewrite/deployments?environment=development#activity-log'",
    "test": "jest"
  },
  "packageManager": "yarn@3.6.4",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
